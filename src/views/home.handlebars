<hr>
<h1>Lista de Productos</h1>

<div>
  {{#each products}}
  <div>
    <hr>
    <img src="{{thumbnail}}" alt="{{title}}" width="100" height="200">
    <h3>{{title}}</h3>
    <p>{{description}}</p>
    <p>Precio: $ {{price}}</p>
    <p>Stock: {{stock}}</p>
    <br>
    <button id="{{id}}">Add to Cart</button>
    <br>
  </div>
  {{/each}}
</div>

<hr>